# API Keys
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...

# Default Model
# Format: provider:model-id
ALIGNMENTER_DEFAULT_MODEL=openai:gpt-4o-mini

# Embedding Provider
# Examples: sentence-transformer:all-MiniLM-L6-v2, openai:text-embedding-3-small
ALIGNMENTER_EMBEDDING_PROVIDER=

# Judge Provider for Safety Scoring
# Example: openai:gpt-4o-mini
ALIGNMENTER_JUDGE_PROVIDER=

# Judge Budget (max LLM calls per run)
ALIGNMENTER_JUDGE_BUDGET=

# Default Paths (optional - override built-in defaults)
# ALIGNMENTER_DEFAULT_DATASET=datasets/demo_conversations.jsonl
# ALIGNMENTER_DEFAULT_PERSONA=configs/persona/default.yaml
# ALIGNMENTER_DEFAULT_KEYWORDS=configs/safety_keywords.yaml
